msgstr "Товщина нижнього шару підкладки"

msgctxt "raft_base_wall_count label"
msgid "Raft Base Wall Count"
msgstr "Лічильник стін основи підкладки"

msgctxt "raft_margin label"
msgid "Raft Extra Margin"
msgstr "Додаткове поле підкладки"

msgctxt "raft_fan_speed label"
msgid "Raft Fan Speed"
msgstr "Швидкість вентилятора для підкладки"

msgctxt "raft_interface_extruder_nr label"
